{
  "name": "@sourcegraph/jetbrains",
  "description": "Khulnasoft for JetBrains",
  "version": "0.0.0",
  "dependencies": {
    "@sourcegraph/branded": "1.0.0",
    "@sourcegraph/common": "0.0.1",
    "@sourcegraph/http-client": "0.0.1",
    "@sourcegraph/shared": "1.0.0",
    "@sourcegraph/wildcard": "0.0.1"
  },
  "devDependencies": {
    "@sourcegraph/build-config": "0.0.1",
    "@sourcegraph/eslint-plugin-wildcard": "1.0.0",
    "@sourcegraph/storybook": "0.0.1"
  },
  "displayName": "Khulnasoft",
  "license": "Apache-2.0",
  "private": true,
  "publisher": "sourcegraph",
  "repository": {
    "type": "git",
    "url": "https://github.com/khulnasoft/khulnasoft.git",
    "directory": "client/jetbrains"
  },
  "scripts": {
    "build": "ts-node -T scripts/build.ts",
    "lint": "pnpm run lint:js && pnpm run lint:css",
    "lint:css": "stylelint 'webview/**/*.scss'",
    "lint:js": "eslint --cache 'webview/**/*.[jt]s?(x)'",
    "standalone": "ts-node standalone/src/server.ts",
    "storybook": "STORIES_GLOB='client/jetbrains/webview/src/**/*.story.tsx' pnpm --filter @sourcegraph/storybook run start",
    "typecheck": "tsc -b",
    "watch": "WATCH=1 pnpm run build"
  },
  "sideEffects": false
}
